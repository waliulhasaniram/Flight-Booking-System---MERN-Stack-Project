{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds" : [
        {
            "src" : "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/**",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest":"/"}
    ]
}